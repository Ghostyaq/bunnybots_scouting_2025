          "type": "select",
          "required": true,
          "code": "robot",
          "formResetBehavior": "preserve",
          "defaultValue": "R1",
          "choices": {
            "R1": "Red 1",
            "R2": "Red 2",
            "B1": "Blue 1",
            "B2": "Blue 2"
          }
        },
        {
          "title": "Team Number",
          "description": "The team number of the robot you're scouting.",
          "type": "select",
          "required": true,
          "code": "teamNumber",
          "formResetBehavior": "reset",
          "defaultValue": "",
          "choices": {
            "449": "449",
            "611": "611",
            "686": "686",
            "1915": "1915",
            "2537": "2537",
